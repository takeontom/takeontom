{
  "extends": "airbnb-base",
  "rules": {
    "max-len": [
      "error",
      79,
      2,
      {
        "ignoreUrls": true,
        "ignoreComments": false,
        "ignoreRegExpLiterals": true,
        "ignoreStrings": false,
        "ignoreTemplateLiterals": false
      }
    ],
    "function-paren-newline": "off"
  }
}
